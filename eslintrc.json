{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base","prettier"
    ],

    "plugins":["prettier"],
  
   
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "no-console": 0,
        "no-unused-vars": "warn",
        "no-debugger": 0,
        "no-alert": 0,
        "quotes": [
            1,
            "single",
            {
              "avoidEscape": true,
              "allowTemplateLiterals": true
            }
          ],
          "prettier/prettier":[
            "error",
            {
                "endOfLine": "auto"
            }
          ]
       

    }

}
